{
    "LineLength": 140,
    "Linters": {
        "vet-nocomposites": "go vet -composites=false:^(?:vet:.*?\\.go:\\s+(?P<path>.*?\\.go):(?P<line>\\d+):(?P<col>\\d+):\\s*(?P<message>.*))|(?:(?P<path>.*?\\.go):(?P<line>\\d+):\\s*(?P<message>.*))$"
    },
    "MinConfidence": 0.3,
    "MinOccurrences": 2,
    "Deadline": "600s",
    "WarnUnmatchedDirective": true,
    "Enable": [
        "vet-nocomposites",
        "vetshadow",
        "megacheck",
        "goimports",
        "golint",
        "structcheck",
        "unconvert",
        "goconst",
        "interfacer",
        "misspell",
        "ineffassign",
        "varcheck",
        "deadcode",
        "unparam",
        "gofmt",
        "goimports"
    ]
}
